<h1>Sign Up</h1>
<h3><%= render partial: "layouts/errors", locals: { object: @user } %></h3>
<%=form_for @user do |f|%>
<%=f.label :username%>
<%=f.text_field :username%>

<%=f.label :email%>
<%=f.email_field :email%>

<%=f.label :password%>
<%=f.password_field :password%>

<%=f.submit "Sign Up"%>

<%end%>

<br> OR 
<%= link_to "Log In with Google", '/auth/google_oauth2' %>